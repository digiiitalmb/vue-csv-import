{
    "name": "vue-csv-import",
    "version": "4.0.1",
    "private": false,
    "description": "Vue.js component to handle CSV uploads with field mapping.",
    "author": "John Gile <me@jgile.com>",
    "main": "dist/vue-csv-import.umd.min.js",
    "unpkg": "dist/vue-csv-import.umd.min.js",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --target lib --name vue-csv-import ./src/index.js --dest ./dist && rm ./dist/demo.html",
