    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.4",
        "@vue/test-utils": "^2.0.0-0",
        "typescript": "~3.9.3",
        "vue-jest": "^5.0.0-0",
        "core-js": "^3.6.5",
        "vue": "^3.0.4"
    },
    "files": [
        "dist/*.js"
    ],
    "bugs": {
        "url": "https://github.com/jgile/vue-csv-import/issues"
    },
    "keywords": [
        "jgile",
